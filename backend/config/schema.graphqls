type Error{
    code:String
    message:String
    stackTrace:String

}
type User{
    id:ID,
    username: String,
    password: String,
    emailAddress: String,
    firstName: String,
    lastName: String,
    phone: String,
    address: String,
    city: String,
    zipCode: String,
    country: String,
    createdAt: String,
    userRoles: [UserRole]
    isActive: Boolean,
}
type UserRole{
    id:ID,
    userId: ID,
    roleId: ID,
    name: String,
    createdAt: String
}
input UserRoleInput{
    userId: ID,
    roleId: ID,
    name: String
}
